unit App;
{ *************************************************************************** }
interface
{ *************************************************************************** }
uses
  App.Console;
{ *************************************************************************** }
type
  TApp = class
    private
      FConsole : TConsole;
    public
      constructor Create;
      destructor  Destroy; override;
  end;
{ *************************************************************************** }
implementation
{ *************************************************************************** }
uses
  SysUtils;
{ *************************************************************************** }
{ PUBLIC                                                                      }
{ *************************************************************************** }
constructor TApp.Create;
begin
  FConsole := TConsole.Create;
end;
{ *************************************************************************** }
destructor TApp.Destroy;
begin
  FreeAndNil(FConsole);
  inherited;
end;
{ *************************************************************************** }
end.
